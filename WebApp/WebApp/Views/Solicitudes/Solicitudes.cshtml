@model WebApp.Models.SolicitudServicio

@{
    ViewData["Title"] = "Detalles Solicitud";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h1>Detalles Solicitud</h1>

<div>
    <h4>Detalle</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Código
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdUsuario)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario.primer_nombre + model.Usuario.primer_apellido)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Servicio.Servicio_Descripcion)
        </dd>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Servicio.Servicio_FechaInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Servicio.Servicio_FechaCierre)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Servicio.Servicio_Costo)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Servicio.Estado)
        </dt>

    </dl>
</div>
<div>
   
    <a asp-action="Index">Regresar a la lista</a>
</div>
